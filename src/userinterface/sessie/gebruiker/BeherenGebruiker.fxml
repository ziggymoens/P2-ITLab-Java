<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="250.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox fx:id="hBoxOnderaan1" alignment="CENTER_LEFT" prefHeight="250.0" prefWidth="1400.0">
            <children>
                <VBox fx:id="vBoxOnderLinks1" alignment="CENTER" prefHeight="250.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="txtFieldSearchbar" promptText="Zoek op naam" />
                  <Button fx:id="btnZoeken" mnemonicParsing="false" prefHeight="27.0" prefWidth="119.0" text="Zoek">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                    </children>
                    <HBox.margin>
                        <Insets left="50.0" />
                    </HBox.margin>
                </VBox>
                <TableView fx:id="tableViewGebruiker" prefHeight="250.0" prefWidth="900.0">
                    <HBox.margin>
                        <Insets left="50.0" />
                    </HBox.margin>
                    <columns>
                        <TableColumn fx:id="TCnaam" prefWidth="300.0" text="Naam gebruiker" />
                        <TableColumn fx:id="TCtype" prefWidth="300.0" text="Type" />
                        <TableColumn fx:id="TCstatus" prefWidth="300.0" text="Status" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
                <Button fx:id="btnKiezen" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="81.0" prefWidth="55.0" text="Kiezen">
                    <HBox.margin>
                        <Insets left="50.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</AnchorPane>
