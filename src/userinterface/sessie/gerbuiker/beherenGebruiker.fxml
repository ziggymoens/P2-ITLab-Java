<AnchorPane prefHeight="250.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userinterface.sessie.gerbuiker.BeherenGebruiker">
   <children>
      <HBox fx:id="hBoxOnderaan1" alignment="CENTER_LEFT" prefHeight="250.0" prefWidth="1400.0">
