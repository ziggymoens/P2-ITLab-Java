<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="apGebruiker" prefHeight="250.0" prefWidth="1400.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="hBoxOnderaan" alignment="CENTER" prefHeight="250.0" prefWidth="1400.0">
         <children>
            <VBox fx:id="vBoxOnderLinks" alignment="CENTER" prefHeight="215.0" prefWidth="243.0">
               <children>
                  <ComboBox fx:id="choiceBox1Onder" prefWidth="200.0" promptText="Stad">
                     <VBox.margin>
                        <Insets right="20.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="choiceBox2Onder" prefWidth="200.0" promptText="Gebouw">
                     <VBox.margin>
                        <Insets right="20.0" top="20.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="choiceBox3Onder" prefWidth="200.0" promptText="Verdieping">
                     <VBox.margin>
                        <Insets right="20.0" top="20.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="choiceBox4Onder" prefWidth="200.0" promptText="Capaciteit">
                     <VBox.margin>
                        <Insets right="20.0" top="20.0" />
                     </VBox.margin>
                  </ComboBox>
               </children>
            </VBox>
            <TableView fx:id="tableViewOnderaan" prefHeight="215.0" prefWidth="809.0">
               <columns>
                  <TableColumn prefWidth="210.0" text="Lokaal" />
                  <TableColumn prefWidth="150.0" text="Stad" />
                  <TableColumn prefWidth="150.0" text="Gebouw" />
                  <TableColumn prefWidth="150.0" text="Verdieping" />
                  <TableColumn prefWidth="150.0" text="Capaciteit" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="btnKiezen" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="81.0" prefWidth="55.0" text="Kiezen">
               <HBox.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</fx:root>
