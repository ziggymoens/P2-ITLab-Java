                            <HBox.margin>
                                <Insets right="30.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="cancel" mnemonicParsing="false" prefWidth="150.0" text="Cancel">
                            <HBox.margin>
                                <Insets left="30.0"/>
                            </HBox.margin>
