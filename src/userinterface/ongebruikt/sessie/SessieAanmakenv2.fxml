                                        <TextField fx:id="naamGast" editable="false">
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </VBox.margin>
                                        </TextField>
                                        <Label fx:id="foutGast" alignment="CENTER" prefWidth="350.0" textFill="RED">
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0"/>
                                            </VBox.margin>
                                        </Label>
                                    </children>
                                </VBox>
                                <VBox>
                                    <children>
                                        <Label text="Maximum aantal plaatsen">
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </VBox.margin>
                                        </Label>
                                        <TextField fx:id="maxPlaatsen" editable="false">
