<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root prefHeight="650.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <BorderPane layoutY="250.0" prefHeight="400.0" prefWidth="600.0">
            <center>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER" prefHeight="164.0" prefWidth="261.0">
                                    <children>
                                        <Label text="Media"/>
                                        <ListView fx:id="listViewMedia" minHeight="100.0" minWidth="270.0"
                                                  prefHeight="100.0" prefWidth="270.0"/>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                            <children>
                                                <Button fx:id="infoMedia" mnemonicParsing="false" text="info">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="nieuwMedia" alignment="CENTER" mnemonicParsing="false"
                                                        text="Nieuw">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="bewerkenMedia" mnemonicParsing="false" text="Bewerken">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                            </children>
                                            <opaqueInsets>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </opaqueInsets>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </VBox>
                                <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="200.0"
                                      prefWidth="100.0">
                                    <children>
                                        <Label text="Feedback"/>
                                        <ListView fx:id="listViewFeedback" minHeight="100.0" minWidth="270.0"
                                                  prefHeight="200.0" prefWidth="270.0"/>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                            <children>
                                                <Button fx:id="infoFeedback" mnemonicParsing="false" text="info">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="nieuwFeedback" mnemonicParsing="false" text="Nieuw">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="bewerkenFeedback" mnemonicParsing="false"
                                                        text="Bewerken">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                            </children>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </VBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER" minWidth="270.0" prefWidth="270.0">
                                    <children>
                                        <Label text="Inschrijvingen"/>
                                        <ListView fx:id="listViewInschrijvingen" minHeight="100.0" minWidth="225.0"
                                                  prefHeight="200.0" prefWidth="200.0"/>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                            <children>
                                                <Button fx:id="infoInschrijving" contentDisplay="CENTER"
                                                        mnemonicParsing="false" text="info">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="nieuwInschrijving" mnemonicParsing="false" text="Nieuw">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="bewerkenInschrijving" mnemonicParsing="false"
                                                        text="Bewerken">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                            </children>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </VBox>
                                <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="200.0"
                                      prefWidth="100.0">
                                    <children>
                                        <Label text="Aankondigingen"/>
                                        <ListView fx:id="listViewAankondigingen" minHeight="100.0" minWidth="270.0"
                                                  prefHeight="200.0" prefWidth="270.0"/>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                            <children>
                                                <Button fx:id="infoAankondiging" mnemonicParsing="false" text="info">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="voegAankondigingToe" mnemonicParsing="false"
                                                        text="Nieuw">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="bewerkenAankondiging" mnemonicParsing="false"
                                                        text="Bewerken">
                                                    <HBox.margin>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                                    </HBox.margin>
                                                </Button>
                                            </children>
                                        </HBox>
                                    </children>
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </VBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </center>
        </BorderPane>
        <HBox alignment="CENTER" layoutX="15.0" layoutY="6.0" prefHeight="242.0" prefWidth="571.0">
            <children>
                <VBox prefHeight="240.0" prefWidth="260.0">
                    <children>
                        <Label text="Sessie ID">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="sessieId" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label text="Titel">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="titel" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label text="Naam Gastspreker">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="naamGast" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label text="Maximum aantal plaatsen">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="maxPlaatsen" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label text="Lokaal">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="lokaal" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                    </children>
                    <HBox.margin>
                        <Insets left="5.0" right="5.0"/>
                    </HBox.margin>
                </VBox>
                <VBox prefHeight="240.0" prefWidth="260.0">
                    <children>
                        <Label text="Start van de sessie">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="start" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label text="Einde van de sessie">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="eind" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label text="Verantwoordelijke">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="verantwoordelijke" editable="false">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <CheckBox fx:id="geopend" mnemonicParsing="false" text="Sessie Geopend">
                            <VBox.margin>
                                <Insets bottom="5.0" left="10.0" right="5.0" top="12.0"/>
                            </VBox.margin>
                        </CheckBox>
                    </children>
                    <HBox.margin>
                        <Insets left="5.0" right="5.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
    </children>
</fx:root>
