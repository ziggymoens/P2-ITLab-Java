<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root prefHeight="800.0" prefWidth="1400.0" type="BorderPane" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox alignment="CENTER" BorderPane.alignment="TOP_CENTER">
            <children>
                <HBox alignment="TOP_CENTER" prefWidth="1200.0">
                    <children>
                        <Label fx:id="schermTitel" alignment="CENTER" text="Bewerken" underline="true">
                            <font>
                                <Font name="Arial Bold" size="32.0"/>
                            </font>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Label>
                    </children>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </HBox>
                <HBox>
                    <children>
                        <VBox alignment="CENTER_LEFT" prefWidth="300.0">
                            <children>
                                <Label text="Titel">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="titel" editable="false">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <Label text="Naam Gastspreker">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="naamGast" editable="false">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <Label text="Maximum aantal plaatsen">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="maxPlaatsen" editable="false">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <Label text="Lokaal">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <ChoiceBox fx:id="lokaal" prefWidth="245.0">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="10.0" top="5.0"/>
                                    </VBox.margin>
                                </ChoiceBox>
                                <Label text="Start van de sessie">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="start" editable="false">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <Label text="Einde van de sessie">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="eind" editable="false">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <Label text="Verantwoordelijke">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="verantwoordelijke" editable="false">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </TextField>
                                <CheckBox fx:id="geopend" mnemonicParsing="false" text="Sessie Geopend">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="10.0" right="5.0" top="12.0"/>
                                    </VBox.margin>
                                </CheckBox>
                            </children>
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" prefWidth="200.0">
                            <children>
                                <VBox alignment="CENTER_LEFT">
                                    <children>
                                        <Label text="Inschrijvingen"/>
                                        <HBox alignment="CENTER_RIGHT" prefHeight="65.0">
                                            <children>
                                                <Label fx:id="lblinschrijvingen" text="Aantal:">
                                                    <HBox.margin>
                                                        <Insets right="15.0"/>
                                                    </HBox.margin>
                                                </Label>
                                                <Button fx:id="inschrijvingen" mnemonicParsing="false" text="Toon"/>
                                            </children>
                                        </HBox>
                                    </children>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </VBox>
                                <VBox>
                                    <children>
                                        <Label text="Aankondigingen"/>
                                        <HBox alignment="CENTER_RIGHT" prefHeight="65.0">
                                            <children>
                                                <Label fx:id="lblaankondigingen" text="Aantal:">
                                                    <HBox.margin>
                                                        <Insets right="15.0"/>
                                                    </HBox.margin>
                                                </Label>
                                                <Button fx:id="aankondigingen" mnemonicParsing="false" text="Toon"/>
                                            </children>
                                        </HBox>
                                    </children>
                                    <VBox.margin>
                                        <Insets top="20.0"/>
                                    </VBox.margin>
                                </VBox>
                                <VBox>
                                    <children>
                                        <Label text="Media"/>
                                        <HBox alignment="CENTER_RIGHT" prefHeight="65.0">
                                            <children>
                                                <Label fx:id="lblmedia" text="Aantal:">
                                                    <HBox.margin>
                                                        <Insets right="15.0"/>
                                                    </HBox.margin>
                                                </Label>
                                                <Button fx:id="media" mnemonicParsing="false" text="Toon"/>
                                            </children>
                                        </HBox>
                                    </children>
                                    <VBox.margin>
                                        <Insets top="20.0"/>
                                    </VBox.margin>
                                </VBox>
                                <VBox>
                                    <children>
                                        <Label text="Feedback"/>
                                        <HBox alignment="CENTER_RIGHT" prefHeight="65.0">
                                            <children>
                                                <Label fx:id="lblfeedback" text="Aantal:">
                                                    <HBox.margin>
                                                        <Insets right="15.0"/>
                                                    </HBox.margin>
                                                </Label>
                                                <Button fx:id="feedback" mnemonicParsing="false" text="Toon"/>
                                            </children>
                                        </HBox>
                                    </children>
                                    <VBox.margin>
                                        <Insets top="20.0"/>
                                    </VBox.margin>
                                </VBox>
                            </children>
                            <HBox.margin>
                                <Insets left="30.0" right="30.0"/>
                            </HBox.margin>
                        </VBox>
                        <VBox fx:id="vboxTable" alignment="CENTER" maxHeight="1.7976931348623157E308"
                              maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="800.0"/>
                    </children>
                    <VBox.margin>
                        <Insets bottom="30.0" left="10.0" right="10.0" top="30.0"/>
                    </VBox.margin>
                </HBox>
                <HBox alignment="CENTER">
                    <children>
                        <Button fx:id="save" mnemonicParsing="false" prefWidth="110.0" text="Save">
                            <HBox.margin>
                                <Insets right="30.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="cancel" mnemonicParsing="false" prefWidth="110.0" text="Cancel">
                            <HBox.margin>
                                <Insets left="30.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </center>
</fx:root>
