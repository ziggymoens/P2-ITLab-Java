<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="700.0" prefWidth="1200.0" type="BorderPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox alignment="CENTER_LEFT" prefHeight="540.0" prefWidth="150.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="362.0" prefWidth="150.0">
               <children>
                  <Label text="Filter:" />
                  <CheckBox mnemonicParsing="false" text="niet geopende sessies">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </CheckBox>
                  <CheckBox mnemonicParsing="false" text="geopende sessies">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </CheckBox>
                  <CheckBox mnemonicParsing="false" text="open sessies">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </CheckBox>
                  <CheckBox mnemonicParsing="false" text="Aalst">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </CheckBox>
                  <CheckBox mnemonicParsing="false" text="Gent">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </CheckBox>
               </children>
               <padding>
                  <Insets left="5.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </left>
   <center>
      <VBox maxWidth="1100.0" prefHeight="690.0" prefWidth="1100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="195.0" prefWidth="1050.0">
               <children>
                  <VBox alignment="CENTER_LEFT" prefHeight="144.0" prefWidth="232.0">
                     <children>
                        <Label text="Selecteer een kalenderjaar:" />
                        <ChoiceBox fx:id="choiceBoxSessie" accessibleRole="CHECK_BOX" prefWidth="200.0">
                           <VBox.margin>
                              <Insets bottom="20.0" left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                        </ChoiceBox>
                     </children>
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </VBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="144.0" prefWidth="550.0">
                     <children>
                        <TextField maxWidth="500.0" prefHeight="26.0" prefWidth="409.0" promptText="Zoek sessie naam" />
                        <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="55.0" text="Zoek">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <VBox alignment="CENTER_LEFT" prefHeight="581.0" prefWidth="1050.0">
               <children>
                  <TableView fx:id="tableView" prefHeight="482.0">
                    <columns>
                      <TableColumn prefWidth="300.0" text="Titel" />
                      <TableColumn prefWidth="200.0" text="Start sessie" />
                        <TableColumn prefWidth="200.0" text="Einde sessie" />
                        <TableColumn fx:id="lastColumn" prefWidth="200.0" text="Vrije plaatsen" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <VBox.margin>
                        <Insets right="20.0" />
                     </VBox.margin>
                  </TableView>
                  <HBox alignment="CENTER" prefHeight="57.0" prefWidth="1050.0">
                     <children>
                      <Button fx:id="nieuweSessie" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefWidth="120.0" text="Nieuwe Sessie">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="verwijderSessie" mnemonicParsing="false" prefWidth="120.0" text="Verwijder Sessie">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </center>
</fx:root>
