<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<fx:root prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" type="BorderPane" fx:controller="userinterface.gui.rommel.details.DetailsController">
   <center>
      <VBox fx:id="detailsVBox" prefHeight="100.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="detailsEdit" mnemonicParsing="false" text="EDIT" />
            <Button fx:id="detailsDelete" mnemonicParsing="false" text="DELETE" />
        </buttons>
      </ButtonBar>
   </bottom>
</fx:root>
