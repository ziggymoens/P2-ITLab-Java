<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="738.0" prefWidth="1400.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="738.0" prefWidth="600.0" AnchorPane.leftAnchor="50.0">
         <children>
            <Label text="Academiejaren">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="tableAj" prefHeight="600.0" prefWidth="500.0">
               <columns>
                  <TableColumn fx:id="academiejaarString" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" text="Academiejaar" />
                  <TableColumn fx:id="startString" prefWidth="75.0" text="Start Datum" />
                  <TableColumn fx:id="eindString" prefWidth="75.0" text="Eind Datum" />
                  <TableColumn fx:id="aantalSessies" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="Sessies" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="738.0" prefWidth="600.0" AnchorPane.rightAnchor="50.0">
         <children>
            <Label text="Sessies">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="tableSessie" prefHeight="600.0" prefWidth="600.0">
              <columns>
                  <TableColumn fx:id="titel" maxWidth="275.0" minWidth="275.0" prefWidth="275.0" text="Titel" />
                <TableColumn fx:id="datum" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" sortable="false" text="Datum" />
                  <TableColumn fx:id="stad" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Stad" />
                <TableColumn fx:id="aanwezigen" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="Aanwezigen" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
</fx:root>
